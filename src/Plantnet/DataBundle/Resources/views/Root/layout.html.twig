<!DOCTYPE html>
<!--[if lt IE 7 ]> <html class="ie6"> <![endif]-->
<!--[if IE 7 ]> <html class="ie7"> <![endif]-->
<!--[if IE 8 ]> <html class="ie8"> <![endif]-->
<!--[if IE 9 ]> <html class="ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html> <!--<![endif]-->
<head>
	{% block head %}
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		{#<!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame
		Remove this if you use the .htaccess -->#}
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
		<meta name="description" content=""/>
		<meta name="author" content="Barbe Julien (AMAP), Affouard Antoine (AMAP)"/>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		{#<!-- Le HTML5 shim, for IE6-8 support of HTML elements -->#}
		<!--[if lt IE 9]>
			<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
		<![endif]-->
		<link type="image/x-icon" rel="shortcut icon" href="{{ asset('bundles/plantnetdata/images/favicon.ico') }}" />
		<title>{% block title %}Pl@ntnet{% endblock %}</title>
		{% block stylesheets %}
			{% stylesheets
				filter='less,cssrewrite,?yui_css'
				output='css/publish_root.css'
				debug=false
				'bundles/plantnetdata/bootstrap/css/less/bootstrap.less'
				'bundles/plantnetdata/css/root/styles.less'
				'bundles/plantnetdata/css/root/map.less'
				'bundles/plantnetdata/bootstrap/css/less/responsive.less'
				'bundles/plantnetdata/css/root/styles-responsive.less'
			%}
			<link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
			{% endstylesheets %}
		{% endblock %}
	{% endblock %}
</head>
<body>
	{% block topbar %}
		<div class="navbar navbar-fixed-top">
			<div class="navbar-inner">
				<div class="container">
					<a class="brand" href="{{ path('front_index') }}">
						<img src="{{ asset('bundles/plantnetdata/images/plantnet.png') }}" alt="Pl@ntNet - Publish" />
					</a>
				</div>
			</div>
		</div>
	{% endblock %}
	<div class="container">
		<div class="content">
			{% block header %}{% endblock %}
			<div class="row">
				<div class="span12">
					{% block breadcrumb %}{% endblock %}
					{% block row %}
						<div class="row">
							<div class="span3">
								{% block side %}{% endblock %}
							</div>
							<div class="span9" id="data-load">
								{% block main %}{% endblock %}
							</div>
						</div>
					{% endblock %}
				</div>
			</div>
		</div>
	</div>
	<footer id="footer">
		<div class="container">
			{% block footer %}
				<ul>
					<li class="logofoot">
						<a href="http://www.plantnet.org" target="_blank">
							<img src="{{ asset('bundles/plantnetdata/images/footPlantnet.png') }}" alt="Projet Plantnet"></a>
					</li>
					<li class="logofoot">
						<a href="http://www.agropolis-fondation.fr/" target="_blank">
							<img src="{{ asset('bundles/plantnetdata/images/footAF.png') }}" alt="Agropolis Fondation"></a>
					</li>
				</ul>
			{% endblock %}
		</div>
	</footer>
	{% block javascripts %}
		{% javascripts
			'bundles/plantnetdata/js/jquery-1.7.min.js'
			'bundles/plantnetdata/bootstrap/js/bootstrap.min.js'
			'bundles/plantnetdata/js/spin.min.js'
			filter='?yui_js' output="js/publish.js" debug=false %}
			<script src="{{ asset_url }}"></script>
		{% endjavascripts %}
		<script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
		<script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
		{% include "PlantnetDataBundle:Analytics:analyticstracking.html.twig" %}
	{% endblock %}
</body>
</html>